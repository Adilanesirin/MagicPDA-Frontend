{
  "expo": {
    "name": "TaskPMS",
    "slug": "IMCSync",
    "owner": "adila-2005",
    "version": "3.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/task-pms-adaptive-foreground-1024.png",
    "scheme": "taskpms",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "primaryColor": "#FF8C00",
    "backgroundColor": "#FFFFFF",
    "splash": {
      "image": "./assets/images/icon-foreground.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "url": "https://u.expo.dev/ee603ddc-2674-4167-aea0-dabe67fd77e3",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taskpms.newapp",
      "buildNumber": "4",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true,
          "NSExceptionDomains": {
            "localhost": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSExceptionRequiresForwardSecrecy": false
            }
          }
        }
      }
    },
    "android": {
      "package": "com.taskpms.newapp",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/task-pms-adaptive-foreground-1024-fixed.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_PHONE_STATE"
      ],
      "allowBackup": false,
      "requestLegacyExternalStorage": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/BG-fix.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-sqlite",
      "./plugin/withNetworkConfig"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f25ceba9-3d51-4b1f-9ca4-3986a0095dd0"
      }
    }
  }
}